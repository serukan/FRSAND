<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ozelyapim.frs" version="1.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>FRS Gaz</name>
    <description>
        FRS Gaz Mobil Aplikasyonu
    </description>
    <author email="info@ozelyapim.com.tr" href="http://www.ozelyapim.com.tr">
        Özel Yapım Reklam Ajansı
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
